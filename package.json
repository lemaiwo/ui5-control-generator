{
  "name": "ui5-control-generator",
  "version": "1.0.1",
  "description": "UI5 Control generator based on html",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "try": "npm run build && node dist/index.js --loglevel debug --p controls --s true --ns be.wl --ow true",
    "try-watch": "npm run build && node dist/index.js --watch",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint src",
    "ci": "npm-run-all build test lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemaiwo/ui5-control-generator.git"
  },
  "keywords": [
    "UI5",
    "SAPUI5",
    "OpenUI5",
    "Controls"
  ],
  "author": "Wouter Lemaire",
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/hjson": "^2.4.3",
    "@types/html2json": "^1.0.1",
    "@types/jest": "^27.0.3",
    "@types/jquery": "^3.5.14",
    "@types/node": "^16.11.12",
    "@types/openui5": "^1.97.0",
    "@types/yargs": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "jest": "^27.4.4",
    "npm-run-all": "4.1.5",
    "ts-jest": "^27.1.1",
    "typescript": "4.5.3"
  },
  "dependencies": {
    "glob": "^8.0.3",
    "hjson": "^3.2.2",
    "html2json": "^1.0.2",
    "jquery": "^3.6.0",
    "loglevel": "1.8.0",
    "yargs": "^17.3.1"
  },
  "files": [
    "dist"
  ]
}
